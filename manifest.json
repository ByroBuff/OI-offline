{
    "manifest_version": 3,
    "name": "Osint Industries Offline",
    "version": "1.0",
    "description": "View and save osint.industries results offline",
    "permissions": [
        "scripting",
        "activeTab"
      ],
      "action": {},
      "content_scripts": [
        {
          "matches": ["https://osint.industries/email"],
          "js": ["content.js"]
        }
      ],
      "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
  }
  